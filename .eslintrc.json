{
    "extends": "airbnb",
    "parser":"babel-eslint",

    "rules":{
        "jsx-ally/anchor-is-valid":0,
        "jsx-ally/label-has-for":0,
        "class-methods-use-this":0,
        "react/jsx-filename-extension":0,
        "prefer-promise-reject-errors":0
    },
    "globals":{
        "localStorage":true,
        "document":true,
        "FormData":true
    }
}